all: out clean

out: main.o  init.o aff.o menu.o regle.o sav.o reseau.o
	gcc main.o  init.o aff.o menu.o regle.o sav.o reseau.o -o okey.exe

main.o: main.c
	gcc main.c -c

init.o: ./Initialisation/init.c
	gcc ./Initialisation/init.c -c

aff.o: ./Affichage/aff.c
	gcc ./Affichage/aff.c -c

menu.c: ./Menu/menu.c 
	gcc ./Menu/menu.c -c

regle.o: ./Regle/regle.c
	gcc ./Regle/regle.c -c

sav.o: ./Sauvegarde/sav.c
	gcc ./Sauvegarde/sav.c -c

reseau.o: ./Reseau/reseau.c
	gcc ./Reseau/reseau.c -c

clean:
	rm *.o
